{
  "name": "contactbookreact",
  "version": "1.0.0",
  "description": "contacts",
  "main": "server.js",
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
