  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "connect-timeout": "^1.9.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "mongoose": "^5.8.9"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  }
}
